<div class="container">
<h1>Clubs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Image url</th>
      <th>Category</th>
      <th>Link to</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clubs.each do |club| %>
      <tr>
        <td><%= club.name %></td>
        <td><%= club.description %></td>
        <td><%= club.image_url %></td>
        <td><%= club.category %></td>
        <td><%= club.link_to %></td>
        <td><%= link_to 'Show', club %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>